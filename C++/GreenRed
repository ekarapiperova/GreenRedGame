#include <iostream>

using namespace std;

//create first grid
void GenerationZero(int x,int y,int a[1000][1000]){
      for (int i = 0; i < x; i++)
    {
      for (int j = 0; j < y; j++)
	{
	  int curr;
	  cin >> curr;
	  a[i][j] = curr;
	  b[i][j]=curr;
	}
    }
}
//return function which count how many green and red elements are around i,j elem
int checkGreenRed(int i,int j,int x, int y,int a[1000][1000]){
       int red = 0;
        int green = 0;
    
	  if (i - 1 >= 0 && j - 1 >= 0)
	    {
	      if (a[i - 1][j - 1] == 0)
		red++;
	      else
		green++;
	    }
	  if (i - 1 >= 0 && j >= 0)
	    {
	      if (a[i - 1][j] == 0)
		red++;
	      else
		green++;
	    }
	  if (i - 1 >= 0 && j + 1 < y)
	    {
	      if (a[i - 1][j + 1] == 0)
		red++;
	      else
		green++;
	    }

	  if (i >= 0 && j - 1 >= 0)
	    {
	      if (a[i][j - 1] == 0)
		red++;
	      else
		green++;
	    

	}
      if (i >= 0 && j + 1 <y)
	{
	  if (a[i][j + 1] == 0)
	    red++;
	  else
	    green++;
	}


      if (i + 1 >= 0 && i + 1 <x && j - 1 >= 0)
	{
	  if (a[i + 1][j - 1] == 0)
	    red++;
	  else
	    green++;
	}
      if (i + 1 >= 0 &&i + 1 <x && j >= 0)
	{
	  if (a[i + 1][j] == 0)
	    red++;
	  else
	    green++;
	}
      if (i + 1 >= 0 && i + 1 <x && j + 1 <y)
	{
	  if (a[i + 1][j + 1] == 0)
	    red++;
	  else
	    green++;
	}
	
	return green;
 };
int main ()
{
    
  int x, y;
  cout<<"x: ";
  cin >> x;
  cout<<"y: ";
  cin >> y;
  int a[1000][1000]={{}};
  int b[1000][1000]={{}};
  
  int x1,y1,N,n;
  int couner=0;
  
GenerationZero(x,y,a);
   cout<<"y1: ";
  cin >> x1;
  cout<<"x1: ";
  cin >> y1;
 
  cout<<"N: ";
  cin >> N;
  
  do{
  
  for(int i=0;i<y;i++){
      for(int j=0;j<x;j++){
         int green=checkGreenRed(i,j,y,x,a);
          if(a[i][j]==0){
              if(green==3 || green==6){
              b[i][j]=1;
          }
          }
          
          if(a[i][j]==1){
              if(green==0 || green==1 ||green==4 || green==5 ||green==7 || green==8){
                  b[i][j]=0;
              }
          }
          
          if(i==y1 && j==x1 && b[i][j]==1)
          couner++;
          
      }
      
  }
  
  //update the array  
  for(int i=0;i<y;i++){
      for(int j=0;j<x;j++){
          a[i][j]=b[i][j];
      }
      cout<<endl;
   }
   
   for(int i=0;i<y;i++){
      for(int j=0;j<x;j++){
          cout<<b[i][j];
      }
      cout<<endl;
   }
   n++;
}while(n<N);
   
   cout<<"Counter"<<couner;
 
  

}
 
  
